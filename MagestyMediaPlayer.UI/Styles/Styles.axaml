<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Theme Colors -->
	<SolidColorBrush x:Key="BackgroundBrush" Color="#1E1E1E" />
	<SolidColorBrush x:Key="ForegroundBrush" Color="#FFFFFF" />
	<SolidColorBrush x:Key="AccentBrush" Color="#1E90FF" />
	<SolidColorBrush x:Key="AccentHoverBrush" Color="#4CA8FF" />
	<SolidColorBrush x:Key="SecondaryBrush" Color="#4A4A4A" />
	<SolidColorBrush x:Key="SelectionBrush" Color="#3A3A3A" />
</ResourceDictionary>